tor and support filtering peerDependencies\n* 2012-11-27 - v0.1.0 - initial release\n\n\n[globule]: https://github.com/cowboy/node-globule\n[micromatch]: https://github.com/jonschlinkert/micromatch\n","maintainers":[{"name":"phated","email":"blaine@iceddev.com"},{"name":"tkellen","email":"tyler@sleekcode.net"}],"time":{"modified":"2023-07-12T04:07:42.940Z","created":"2012-11-27T19:15:16.344Z","0.1.0":"2012-11-27T19:15:17.137Z","0.1.1":"2012-11-27T20:06:09.570Z","0.1.2":"2013-04-07T19:45:40.697Z","0.2.0":"2013-10-09T04:02:09.161Z","0.3.0":"2013-10-09T15:35:42.444Z","1.0.0":"2015-09-27T21:10:07.073Z","1.0.1":"2016-02-09T18:11:50.007Z","2.0.0":"2017-08-18T23:12:06.959Z"},"author":{"name":"Tyler Kellen","url":"http://goingslowly.com/"},"repository":{"type":"git","url":"git://github.com/tkellen/js-matchdep.git"},"users":{"otravers":true,"adrianorosa":true,"tpwk":true,"alfredbez":true,"tunnckocore":true,"fassetar":true,"christopher.urquidi":true,"taraman":true,"yajinn":true,"karlbateman":true,"ugarz":true,"wujipai":true,"flumpus-dev":true},"homepage":"https://github.com/tkellen/js-matchdep","keywords":["package.json","dependencies","devDependencies","peerDependencies"],"bugs":{"url":"https://github.com/tkellen/js-matchdep/issues"},"readmeFilename":"README.md","license":"MIT"}